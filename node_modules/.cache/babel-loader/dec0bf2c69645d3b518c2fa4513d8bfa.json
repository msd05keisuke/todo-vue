ass=\"todo-item\">\n      <div>\n        {{ todo.title }}\n      </div>\n      <div class=\"remove-item\">\n      削除\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"todo-list\",\n  data() {\n    return {\n      newTodo: \"\",\n      idForTodo: 3,\n      todos: [\n        {\n          id: 1,\n          title: \"Finishe Vue Screencast\",\n          complated: false,\n        },\n        {\n          id: 2,\n          title: \"Finishe Vue Screencast\",\n          complated: false,\n        },\n        {\n          id: 3,\n          title: \"Finishe Vue Screencast\",\n          complated: false,\n        },\n      ],\n    };\n  },\n  methods: {\n    addTodo() {\n      if (this.newTodo.trim().length == 0) {\n        return;\n      }\n      this.idForTodo++;\n      this.todos.push({\n        id: this.idForTodo,\n        title: this.newTodo,\n        complated: false,\n      });\n\n      this.newTodo = \"\";\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.todo-input {\n  width: 100%;\n  padding: 10px 18px;\n  font-size: 18px;\n  margin-bottom: 16px;\n}\n\n.remove-input {\n  width: 100%;\n  padding: 10px 18px;\n  font-size: 18px;\n  margin-bottom: 16px;\n}\n  \n</style>"],"sourceRoot":"src/components"}]}